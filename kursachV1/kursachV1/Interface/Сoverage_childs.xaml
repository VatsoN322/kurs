                <TextBlock   Margin="3" HorizontalAlignment="Center" FontSize="14" >  
            Программа подбора условий договора страхования по правилам №5</TextBlock >
            </StackPanel>
            <Border Grid.ColumnSpan="5" Margin="3" Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="3"></Border>
            <StackPanel Grid.Row="1" Margin="3" Grid.ColumnSpan="5"  >
                <TextBlock   Margin="3" HorizontalAlignment="Center" FontSize="14" >  
            Входные данные</TextBlock >
            </StackPanel>
            <Border Grid.Row="1" Grid.ColumnSpan="5" Margin="3" Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="3"></Border>

            <Grid Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
