                        <ComboBoxItem>Иностранная валюта</ComboBoxItem>
                    </ComboBox>
                    <ComboBox x:Name="Period" Margin="3" Grid.Row="5" MinHeight="25" MaxWidth="130" DropDownClosed="Period_DropDownClosed">
                        <ComboBoxItem>единовременно</ComboBoxItem>
                        <ComboBoxItem>ежегодно</ComboBoxItem>
                        <ComboBoxItem>ежеквартально</ComboBoxItem>
                        <ComboBoxItem>ежемесячно</ComboBoxItem>
                    </ComboBox>
                </Grid>
                <Grid >
