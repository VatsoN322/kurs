                </DockPanel>
                <DockPanel Grid.Row="2">
                    <CheckBox x:Name="Chinvalid" Margin="3" Checked="CheckBox_Checked_1" Unchecked="CheckBox_Unchecked_1"></CheckBox>
                    <TextBlock HorizontalAlignment="Center">Инвалидность</TextBlock>
                </DockPanel>
                <DockPanel Grid.Row="3" >
                    <CheckBox x:Name="ChVred_zdarov" Margin="3" Unchecked="CheckBox_Unchecked_2" Checked="CheckBox_Checked_2"/>
                    <TextBlock HorizontalAlignment="Center">Вред здоровью</TextBlock>
                </DockPanel>
            </Grid>
            <Grid Grid.Column="2" Grid.Row="3" MinWidth="100" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="143*"/>
                    <ColumnDefinition Width="128*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <TextBox x:Name="Dost_voz" Margin="3,0,100,5" TextAlignment="Center" MaxWidth="120" Grid.ColumnSpan="2" TextChanged="Changed_dos_voz"></TextBox>
                <TextBox x:Name="smert"  Grid.Row="1" Margin="3,0,100,5" TextAlignment="Center" MaxWidth="120" Grid.ColumnSpan="2">0</TextBox>
                <TextBox x:Name="invalid" Grid.Row="2" Margin="3,0,100,5" TextAlignment="Center" MaxWidth="120" Grid.ColumnSpan="2">0</TextBox>
                <TextBox x:Name="Vred_zdarov" Grid.Row="3" Margin="3,0,100,5" TextAlignment="Center" MaxWidth="120" Grid.ColumnSpan="2">0</TextBox>
                <Border Grid.Row="1"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7" Margin="0,0,100,0" Grid.ColumnSpan="2" ></Border>
                <Border Grid.Row="2"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7" Margin="0,0,100,0" Grid.ColumnSpan="2" ></Border>
                <Border     Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7" Margin="0,0,100,0" Grid.ColumnSpan="2" ></Border>
                <Border Grid.Row="3"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7" Margin="0,0,100,0" Grid.ColumnSpan="2" ></Border>
            </Grid>
            <Grid Grid.Row="3" Grid.Column="3" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <StackPanel Grid.Column="1" >

                    <TextBlock Grid.Row="5" MinHeight="20" MaxWidth="130" HorizontalAlignment="Center" Margin="3,0,0,-35" TextWrapping="Wrap" > Общий взнос по достижению возраста и по смерти</TextBlock>
                    <TextBox x:Name="Ob_vznos" Margin="0,45,0,0" TextAlignment="Center" MaxWidth="120" Text="0" IsEnabled="False"/>
                </StackPanel>
                <Border   Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7" Grid.Column="1"   />
                <Border  Grid.Column="2"   Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7"   ></Border>
                <StackPanel Grid.Column="2" >
                    <TextBlock Grid.Row="5" MinHeight="20" MaxWidth="150" HorizontalAlignment="Center" TextWrapping="Wrap" > Курс валюты</TextBlock>
                    <TextBox x:Name="kyrs" Margin="3,38,0,0" TextAlignment="Center" MaxWidth="120" Text="0" TextChanged="kyrs_TextChanged"/>
                </StackPanel>
            </Grid>
            <Button Grid.Column="1" Grid.Row="4" Margin="3" MaxHeight="30" MaxWidth="130" RenderTransformOrigin="0.542,0.5" Click="Button_Click_1">Занесение данных</Button>
