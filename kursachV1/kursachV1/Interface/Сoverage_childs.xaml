                </DockPanel>
                <DockPanel Grid.Column="2" Grid.Row="1" MaxHeight="20" MinWidth="50" MaxWidth="20" Margin="3">
                    <TextBox x:Name="month1" MinWidth="40" HorizontalAlignment="Left" TextChanged="month1_TextChanged"/>
                    <ScrollBar x:Name="scr3" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll3" SmallChange="1" Minimum="1"  Maximum="12" MaxHeight="20"/>
                </DockPanel>
