                        <RowDefinition MaxHeight="25" Height="Auto"></RowDefinition>
                        <RowDefinition MaxHeight="25" Height="*"></RowDefinition>
                        <RowDefinition MaxHeight="25" Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock  HorizontalAlignment="Center" >Число лет</TextBlock>
                    <TextBox x:Name="let2" Margin="3"  Grid.Row="1" MinHeight="25"  MaxWidth="70" IsEnabled="False"></TextBox>
                    <Border Grid.Row="1"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="1" Opacity="0.5"></Border>
                    <Border Grid.Row="2"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="3" Opacity="0.5"></Border>
                    <TextBox x:Name="let" Margin="3" Grid.Row="2" MinHeight="25" MaxWidth="70" IsEnabled="False"></TextBox>
                    <ComboBox x:Name="pol" Margin="3" Grid.Row="3" MinHeight="25" MaxWidth="100" DropDownClosed="pol_DropDownClosed">
                    
                    </ComboBox>
