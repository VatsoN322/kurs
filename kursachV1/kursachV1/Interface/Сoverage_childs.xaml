                    <GradientStop Offset="0.4" Color="Cyan" />
                    <GradientStop Offset="1" Color="DarkOrange" />
                </LinearGradientBrush>
            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition  Width="Auto"></ColumnDefinition>
