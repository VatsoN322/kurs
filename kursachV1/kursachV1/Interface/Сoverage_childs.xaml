                    <Border Grid.Row="2"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="3" Opacity="0.5"></Border>
                    <Border Grid.Row="5"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2"  Opacity="0.7" Margin="27,10,10,-10" ></Border>
                    <TextBlock Grid.Row="5" MinHeight="20" HorizontalAlignment="Center" Margin="33,10,38,-10" > Взнос</TextBlock>
                </Grid>
