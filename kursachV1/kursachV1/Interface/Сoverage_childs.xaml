            <Button Grid.Column="0" Grid.Row="4" Margin="3" MaxHeight="30" MaxWidth="130" RenderTransformOrigin="0.542,0.5" Click="Button_Click_2">Обновление Взносов</Button>
            <Button Grid.Column="0" Grid.Row="4" Margin="3" HorizontalAlignment="Right" MaxHeight="30" MaxWidth="130" RenderTransformOrigin="0.542,0.5" Click="Button_Click_YDV">Удаление Взносов</Button>
            <Button Grid.Column="0" Grid.Row="4" Margin="3" MaxHeight="30" MaxWidth="130" RenderTransformOrigin="0.542,0.5" Click="Button_Click_98" HorizontalAlignment="Left">Список Взносов</Button>
           
            <Button Grid.Column="0" Grid.Row="5" Margin="3,20,0,0"  VerticalAlignment="Top" MaxHeight="30" MaxWidth="130" RenderTransformOrigin="0.542,0.5" Click="ClickBlank4" HorizontalAlignment="Left">Договор</Button>
           


