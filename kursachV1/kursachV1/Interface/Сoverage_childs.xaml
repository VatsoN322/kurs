                        <RowDefinition MaxHeight="15"></RowDefinition>
                        <RowDefinition MaxHeight="25" Height="*"></RowDefinition>
                        <RowDefinition MaxHeight="25" Height="*"></RowDefinition>
                        <RowDefinition MaxHeight="20" Height="*"></RowDefinition>
                        <RowDefinition MaxHeight="20" Height="*"></RowDefinition>
                        <RowDefinition MaxHeight="20" Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock Margin="10,0,10,0">Число дней</TextBlock>
                    <TextBox x:Name="dni2" Margin="3" MaxWidth="50" Grid.Row="1" IsEnabled="False"></TextBox>
                    <TextBox x:Name="dni" Margin="3" MaxWidth="50" Grid.Row="2" IsEnabled="False"></TextBox>
                    <Border Grid.Row="1"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="1" Opacity="0.5"></Border>
                    <Border Grid.Row="2"    Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="3" Opacity="0.5"></Border>
                </Grid>
