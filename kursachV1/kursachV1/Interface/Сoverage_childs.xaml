                    <TextBox x:Name="day3" MinWidth="40" HorizontalAlignment="Left" TextChanged="day3_TextChanged"/>
                    <ScrollBar x:Name="scr5" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll5" SmallChange="1" Minimum="1" MaxHeight="20"/>
                </DockPanel>
                <DockPanel Grid.Column="2" Grid.Row="3" MaxHeight="20" MinWidth="50" MaxWidth="20" Margin="3">
                    <TextBox x:Name="month3" MinWidth="40" HorizontalAlignment="Left" TextChanged="month3_TextChanged"/>
                    <ScrollBar x:Name="scr6" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll6" SmallChange="1" Minimum="1" Maximum="12" MaxHeight="20"/>
                </DockPanel>
                <DockPanel Grid.Column="3" Grid.Row="3" MaxHeight="20" MinWidth="50" MaxWidth="20" Margin="3">
                    <TextBox x:Name="god3" MinWidth="40" HorizontalAlignment="Left"  TextChanged="god3_TextChanged"/>
                    <ScrollBar x:Name="scr" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll" SmallChange="1" Minimum="1900"  MaxHeight="20"/>
                </DockPanel>
                <Border Grid.Row="1" Grid.ColumnSpan="4" Grid.RowSpan="4" Margin="3" Padding="5" BorderBrush="Black" BorderThickness="1,2,1,2" CornerRadius="3"></Border>
            </Grid>
            <DockPanel Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="3">
