                    <ScrollBar x:Name="scr2" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll2" SmallChange="1" Minimum="1900"  MaxHeight="20"/>
                </DockPanel>
                <DockPanel Grid.Column="1" Grid.Row="2" MaxHeight="20" MinWidth="50" MaxWidth="20" Margin="3">
                    <TextBox x:Name="data" MinWidth="40" HorizontalAlignment="Left" Width="16" TextChanged="data_TextChanged"/>
                    <ScrollBar x:Name="scr8" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll8" SmallChange="1" Minimum="1" Maximum="30" MaxHeight="20"/>
                </DockPanel>
                <DockPanel Grid.Column="2" Grid.Row="2" MaxHeight="20" MinWidth="50" MaxWidth="20" Margin="3">
                    <TextBox x:Name="month" MinWidth="40" HorizontalAlignment="Left" TextChanged="month_TextChanged"/>
                    <ScrollBar x:Name="scr7" Orientation="Vertical" HorizontalAlignment="Right"  Scroll="OnScroll7" SmallChange="1" Minimum="1" Maximum="12" MaxHeight="20"/>
                </DockPanel>
                <ComboBox x:Name="Godrag" Grid.Column="3" Grid.Row="2" MaxDropDownHeight="150"  MaxHeight="20"  MinWidth="60"  MaxWidth="80" Margin="3" SelectionChanged="Godrag_SelectionChanged"/>
